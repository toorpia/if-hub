{
  "name": "Pump01.EfficiencyIndex",
  "type": "calculation",
  "inputs": ["Pump01.Flow", "Pump01.PowerConsumption"],
  "expression": "(inputs[0] / inputs[1]) * 100",
  "description": "ポンプ01の効率指標（Flow/PowerConsumption × 100）",
  "unit": "%",
  "options": {
    "fillMissingData": "interpolate"
  }
}
